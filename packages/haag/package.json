{
  "name": "meander",
  "version": "1.0.0",
  "description": "Manages all pages not related to the web app tool you are building. Useful for Project management and the basic app menu.",
  "main": "dist/index.js",
  "ts:main": "src/index.ts",
  "typings": "src/index.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "ts-node -T fuse",
    "test": "node_modules/.bin/jest",
    "dist": "node fuse.js dist",
    "clean": "node fuse.js clean",
    "precommit": "lint-staged",
    "typecheck": "node_modules/.bin/tsc -p ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "Hugo Daniel Henriques Oliveira Gomes",
    "email": "mail@hugodaniel.pt"
  },
  "license": "SEE LICENSE IN FILE LICENSE.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoDaniel/gridgenerator.git"
  },
  "keywords": [
    "project",
    "about",
    "pages",
    "management"
  ],
  "bugs": {
    "url": "https://github.com/HugoDaniel/gridgenerator/issues"
  },
  "homepage": "https://github.com/HugoDaniel/gridgenerator#readme",
  "dependencies": {},
  "devDependencies": {}
}
