{
  "name": "gridgenerator",
  "version": "1.0.0",
  "description": "A design web app for non-designers",
  "scripts": {
    "start": "node fuse.js",
    "test": "node fuse.js test",
    "dist": "node fuse.js dist",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "art",
    "paint",
    "web",
    "app",
    "webapp",
    "grid",
    "vector",
    "pattern",
    "ellipse",
    "line",
    "creative",
    "design",
    "tool",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoDaniel/gridgenerator.git"
  },
  "bugs": {
    "url": "https://github.com/HugoDaniel/gridgenerator/issues"
  },
  "homepage": "https://gridgenerator.com",
  "author": "Hugo Daniel Henriques Oliveira Gomes",
  "license": "SEE LICENSE IN FILE LICENSE.md",
  "dependencies": {
    "inferno": "^7.1.11"
  },
  "devDependencies": {
    "fuse-box": "^3.7.1",
    "fuse-test-runner": "^1.0.16",
    "terser": "^3.17.0",
    "uglify-js": "^3.5.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.4000",
    "ts-transform-classcat": "^0.1.1",
    "ts-transform-inferno": "^4.0.2"
  }
}
